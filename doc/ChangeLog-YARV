Local variables:
add-log-time-format: (lambda ()
  (let* ((time (current-time))
	 (diff (+ (cadr time) 32400))
	 (lo (% diff 65536))
	 (hi  (+ (car time) (/ diff 65536))))
  (format-time-string "%Y-%m-%d(%a) %H:%M:%S +900"  (list hi lo) t)))
indent-tabs-mode: t
tab-width: 8
end:
