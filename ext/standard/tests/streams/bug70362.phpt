--TEST--
Bug #70362 (Can't copy() large 'data://' with open_basedir)
--INI--
open_basedir=.
--FILE--
<?php
$temp = tempnam(__DIR__, 'test');
$data = str_repeat('0', 4096);
$data = 'data://plain/text;base64,' . base64_encode($data);
var_dump(copy($data, $temp));
echo file_get_contents($temp);
?>
--EXPECT--
bool(true)

