--TEST--
Feature GH-12143: Extend the maximum precision round can handle by one digit
--INI--
serialize_precisiion=17
--FILE--
<?php

$testCases = [
    'PHP_ROUND_HALF_UP' => [
        [0.12345678901234565, 16],
        [-0.12345678901234565, 16],
        [12345678901234565, -1],
        [-12345678901234565, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
    'PHP_ROUND_HALF_DOWN' => [
        [0.12345678901234565, 16],
        [-0.12345678901234565, 16],
        [12345678901234565, -1],
        [-12345678901234565, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
    'PHP_ROUND_HALF_EVEN' => [
        [0.12345678901234565, 16],
        [-0.12345678901234565, 16],
        [12345678901234565, -1],
        [-12345678901234565, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
    'PHP_ROUND_HALF_ODD' => [
        [0.12345678901234565, 16],
        [-0.12345678901234565, 16],
        [12345678901234565, -1],
        [-12345678901234565, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
    'PHP_ROUND_AWAY_FROM_ZERO' => [
        [0.12345678901234560, 16],
        [-0.12345678901234560, 16],
        [12345678901234567, -1],
        [-12345678901234567, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
    'PHP_ROUND_TOWARD_ZERO' => [
        [0.12345678901234566, 16],
        [-0.12345678901234566, 16],
        [12345678901234565, -1],
        [-12345678901234565, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
    'PHP_ROUND_CEILING' => [
        [0.12345678901234560, 16],
        [-0.12345678901234560, 16],
        [12345678901234564, -1],
        [-12345678901234564, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
    'PHP_ROUND_FLOOR' => [
        [0.12345678901234560, 16],
        [-0.12345678901234560, 16],
        [12345678901234564, -1],
        [-12345678901234564, -1],
        [4503599627370495.5, 0],
        [-4503599627370495.5, 0],
    ],
];

foreach ($testCases as $mode => $cases) {
    echo "========== {$mode} ==========\n";
    foreach ($cases as [$val, $precision]) {
        var_dump(round($val, $precision, constant($mode)));
    }
    echo "\n";
}
?>
