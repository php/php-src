{
    "name": "%VENDORNAME%/%EXTNAME%",
    "type": "php-ext",
    "license": "BSD-3-Clause",
    "description": "Describe your extension here",
    "require": {
        "php": "^8.3"
    },
    "php-ext": {
        "extension-name": "%EXTNAME%",
        "configure-options": [
