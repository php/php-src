--TEST--
Bug GH-8928 (Memory leak with long query/CLOB)
--SKIPIF--
<?php
$target_dbs = array('oracledb' => true, 'timesten' => false);  // test runs on these DBs
require(__DIR__.'/skipif.inc');
?>
--FILE--
<?php

require __DIR__.'/connect.inc';

$sql = 'select CONCAT(CONCAT(TO_CLOB(\'atk__binary__u5f8mzx4vsm8g2c9__\'), TO_CLOB(\'\')), CONCAT(TO_CLOB(\'\'), CONCAT(TO_CLOB(\'\'), TO_CLOB(\'2fb97249b20d0ff6c58506443c8cc9f68543f6e5a4dd295d6f95e7249f18d1ada91166276f5e682398635a9eaa1ba8aeea5b6ab10c4020680afc00ce4dc782f7c2be0\')))) AS "v" from "DUAL"';
$expectedStr = 'atk__binary__u5f8mzx4vsm8g2c9__';

$s = oci_parse($c, $sql);
oci_execute($s);
$row = oci_fetch_array($s, \OCI_ASSOC);
var_dump(array_keys($row));
$res = $row['v']->load();
var_dump(strlen($res));
var_dump($res === $expectedStr);

$mem = null;
$mem = memory_get_usage();
$s = oci_parse($c, $sql);
oci_execute($s);
$row = oci_fetch_array($s, \OCI_ASSOC);
$res = $row['v']->load();
if ($mem !== memory_get_usage()) {
    throw new Exception('Memory usage is expected to be the same');
}
var_dump($res === $expectedStr);

?>
--EXPECTF--
array(1) {
  [0]=>
  string(1) "v"
}
int(4133)
bool(true)
bool(true)
