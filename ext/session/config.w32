ARG_ENABLE("session", "session support", "yes");

if (PHP_SESSION == "yes") {
	EXTENSION("session", "mod_user_class.c session.c mod_files.c mod_mm.c mod_user.c", false /* never shared */, "/DZEND_ENABLE_STATIC_TSRMLS_CACHE=1");
