		if (op1_info & (MAY_BE_ANY-MAY_BE_LONG)) {
			if (op1_info & MAY_BE_DOUBLE) {
				|	IF_NOT_ZVAL_TYPE op1_addr, IS_LONG, >4
			} else {
				|	IF_NOT_ZVAL_TYPE op1_addr, IS_LONG, >9
