			}
			|	ZVAL_DTOR_FUNC var_info, opline
			if (RC_MAY_BE_N(var_info) && (var_info & (MAY_BE_ARRAY|MAY_BE_OBJECT)) != 0) {
				|	jmp >8
				|4:
				|	IF_GC_MAY_NOT_LEAK FCARG1a, eax, >8
				|	EXT_CALL gc_possible_root, r0
			}
			if (in_cold) {
				|	jmp >8
				|.code
			} else {
