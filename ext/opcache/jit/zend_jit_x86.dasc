			|	LOAD_ZVAL_ADDR FCARG2a, op1_addr
			if (Z_REG(res_addr) != ZREG_FCARG1 || Z_OFFSET(res_addr) != 0) {
				|	LOAD_ZVAL_ADDR FCARG1a, res_addr
			}
