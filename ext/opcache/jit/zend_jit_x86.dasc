|.macro EXT_CALL, func, tmp_reg
|	.if X64
||		if (MAY_USE_32BIT_ADDR(func)) {
|			call qword &func
||		} else {
|			LOAD_ADDR tmp_reg, func
|			call tmp_reg
||		}
|	.else
|		call dword &func
|	.endif
|.endmacro

|.macro EXT_JMP, func, tmp_reg
|	.if X64
||		if (MAY_USE_32BIT_ADDR(func)) {
|			jmp qword &func
||		} else {
|			LOAD_ADDR tmp_reg, func
|			jmp tmp_reg
||		}
|	.else
|		jmp dword &func
|	.endif
|.endmacro

