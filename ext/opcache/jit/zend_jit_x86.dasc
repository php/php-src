				|	LOAD_ZVAL_ADDR CARG3, op2_addr
			|.else
				|	sub r4, 12
				|	PUSH_ZVAL_ADDR op2_addr, r0
			|.endif
			|2:
		} else {
			|.if X64
				|	LOAD_ZVAL_ADDR CARG3, op2_addr
			|.else
				|	sub r4, 12
				|	PUSH_ZVAL_ADDR op2_addr, r0
