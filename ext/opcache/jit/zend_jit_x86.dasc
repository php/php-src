				|	mov edx, dword [r0 + offsetof(zend_op_array, num_args)]
				|	// num_args = EX_NUM_ARGS();
				|	mov ecx, dword [FP + offsetof(zend_execute_data, This.u2.num_args)]
				|	// if (UNEXPECTED(num_args > first_extra_arg))
				|	cmp ecx, edx
