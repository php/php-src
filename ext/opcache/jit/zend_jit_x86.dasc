				case ZEND_IS_EQUAL:
				case ZEND_CASE:
					| jne => target_label
					break;
				case ZEND_IS_NOT_EQUAL:
					| je => target_label
					break;
				case ZEND_IS_SMALLER:
					| jge => target_label
					break;
				case ZEND_IS_SMALLER_OR_EQUAL:
					| jg => target_label
					break;
				default:
					ZEND_ASSERT(0);
