			|	test byte [RX + offsetof(zend_execute_data, This.u1.type_info) + 2], (ZEND_CALL_ALLOCATED >> 16)
			|	jnz >1
			|.cold_code
			|1:
			|	mov FCARG1a, RX
			|	EXT_CALL zend_jit_free_call_frame, r0
			|	jmp >1
			|.code
		}
		|	MEM_STORE_ZTS aword, executor_globals, vm_stack_top, RX, r0
		|1:

