static int zend_jit_prologue(dasm_State **Dst)
{
	if (zend_jit_vm_kind == ZEND_VM_KIND_HYBRID) {
		|	SUB_HYBRID_SPAD
	} else if (GCC_GLOBAL_REGS) {
		|	sub sp, sp, SPAD       // stack alignment
	} else {
		|	sub sp, sp, NR_SPAD    // stack alignment
		|	stp FP, RX, T2         // save FP and IP
		|	str LR, T4             // save LR
		|	mov FP, FCARG1x
	}
	return 1;
}

