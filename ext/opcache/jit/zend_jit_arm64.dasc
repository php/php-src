|	.endif
|.endmacro

// Load the value from memory 'addr' into a register 'op'
|.macro MEM_LOAD, ldr_ins, op, addr, tmp_reg
|	LOAD_ADDR tmp_reg, addr
|	ldr_ins op, [tmp_reg]
|.endmacro

|.macro MEM_LOAD_ZTS, ldr_ins, op, struct, field, tmp_reg
|	.if ZTS
