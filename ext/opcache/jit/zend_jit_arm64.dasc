|.macro LOAD_BASE_ADDR, reg, base, offset
||	if (offset) {
||		if (offset > MAX_IMM12) {
|			LOAD_32BIT_VAL reg, offset
|			add reg, Rx(base), reg
||		} else {
|			add reg, Rx(base), #offset
||		}
||	} else {
||		if (base == ZREG_RSP) {
|			mov reg, sp
||		} else {
|			mov reg, Rx(base)
||		}
||	}
|.endmacro

|.macro PUSH_BASE_ADDR, base, offset, tmp_reg
|	brk #0	// TODO
|.endmacro

