// Store the value from a register 'op' into memory 'addr'
|.macro MEM_STORE, str_ins, op, addr, tmp_reg
|	LOAD_ADDR tmp_reg, addr
|	str_ins op, [tmp_reg]
|.endmacro

|.macro MEM_STORE_ZTS, str_ins, op, struct, field, tmp_reg
