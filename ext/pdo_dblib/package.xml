 <maintainers>
  <maintainer>
    <user>wez</user>
    <name>Wez Furlong</name>
    <email>wez@php.net</email>
    <role>lead</role>
  </maintainer>
  <maintainer>
    <user>fmk</user>
    <name>Frank M. Kromann</name>
    <email>fmk@php.net</email>
    <role>lead</role>
