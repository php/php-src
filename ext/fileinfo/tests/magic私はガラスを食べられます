>0	use			netbsd-pure

0	belong&0377777777	043600407	a.out NetBSD/arm32
>0	use			netbsd-normal

# NetBSD/arm26 has always used ELF objects, but it shares a core file
# format with NetBSD/arm32.
0	belong&0377777777	043600507	a.out NetBSD/arm core
>0	use			netbsd-core

