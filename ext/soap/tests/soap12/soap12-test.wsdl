		<!-- 3.4.8 echoFloatArray rpc operation -->
		<operation name="echoFloatArray">
			<soap12:operation/>
			<input>
				<soap12:body use="encoded" namespace="http://example.org/ts-tests" encodingStyle="http://www.w3.org/2003/05/soap-encoding"/>
